0000d4 c01a                      	RJMP Get_state		;Parse the input
                                 	done:
0000d5 cffc                      	RJMP Main			;loop back
                                 
                                 
                                 ;; Transmit function
                                 /**
                                 * @details				Transmit over UART
                                 * @param[in]  out_buf	A byte of data to transmit over UART
                                 * @return				void	
                                 **/
                                 Uart_Tx:
0000d6 9110 00c0                 	LDS r17,UCSR0A		; Load control register
0000d8 ff15                      	SBRS r17,UDRE0		; Check for TX complete
0000d9 cffc                      		RJMP Uart_Tx	; Try again if it's not
0000da 9300 00c6                 	STS UDR0,out_buf	;Write to UDR0 from R16
0000dc 2711                      	CLR r17
0000dd 9508                      	RET
                                 
                                 /**
                                 * @details				Receive over UART
                                 * @param[out]  out_buf	A byte of data received over UART
                                 * @return				void	
                                 **/
                                 Uart_Rx:
0000de 9110 00c0                 	LDS r17,UCSR0A		;Load control register
0000e0 ff17                      	SBRS r17, RXC0		;Check if data is received
0000e1 cffc                      		RJMP Uart_Rx	;Try again if no data
0000e2 9100 00c6                 	LDS out_buf,UDR0    ;Load data from UDR0
0000e4 2711                      	CLR r17				;Clear r17
0000e5 dff0                      	RCALL Uart_Tx		;Echo keypress
0000e6 9508                      	RET
                                 
                                 
                                 /**
                                 * @details					Handles printing the menu
                                 * @param[in]	menu1		The address to the menu
                                 * @param[out]	out_buf		The byte to transmit
                                 * @return					void
                                 **/
                                 Print_menu:
0000e7 e0e0                      	LDI ZL,low(menu1<<1)		;Load the menu address (Needs to be left shifted by 1 to get to the 
0000e8 e0f0                      	LDI ZH,high(menu1<<1)		;correct address)
                                 	Send_byte:				
0000e9 9105                      		LPM out_buf,Z+			;Load data till it hits a null character
0000ea 3000                      		CPI out_buf,0x00		;and send it over UART
0000eb f011                      			BREQ exit
0000ec dfe9                      		RCALL Uart_Tx
0000ed cffb                      		RJMP Send_byte
                                 	exit:
0000ee 9508                      		RET
                                 
                                 /**
                                 * @details					Redirects program based on input
                                 * @param[in]	out_buf		The address to the menu
                                 * @return					void
                                 **/
                                 Get_state:
                                 	L:						;case 'L'
0000ef 340c                      		CPI out_buf,0x4C	;if (out_buf == 'L')
0000f0 f041                      		BREQ Lock			;go to the Lock subroutine
                                 
                                 	U:						;case 'U'
0000f1 3505                      		CPI out_buf,0x55	;if(out_buf == 'U')
0000f2 f071                      		BREQ Ulock			;go to the Unlock subroutine
                                 
                                 	R:						;case 'R'
0000f3 3502                      		CPI out_buf,0x52    ;if(out_buf == 'R')
0000f4 f0c1                      		BREQ RESET			;go to the Reset subroutine
                                 
                                 	Error:							;default
0000f5 eeec                      		LDI ZL,low(error_string<<1)	;Load address of the error message
0000f6 e0f0                      		LDI ZH,high(error_string<<1)
0000f7 dff1                      		RCALL Send_byte				;Print error message
0000f8 cfd9                      		RJMP Main					;Try again
                                 		
                                 /**
                                 * @details				Turns on PORTB pin 1
                                 * @return				void
                                 **/
                                 Lock:	
0000f9 2722                      	CLR r18					;Clear R18
0000fa b925                      	OUT PORTB, r18			;Pull all pins LOW
0000fb e021                      	LDI r18, (1<<0)			;Pull pin 8 on the arduino HIGH
0000fc b925                      	OUT PORTB,r18
0000fd e3e2                      	LDI ZL,low(lock_it<<1)	;Load the lock message address
0000fe e0f1                      	LDI ZH,high(lock_it<<1)
0000ff dfe9                      	RCALL Send_byte			;Print lock message
000100 cfd1                      	RJMP Main				;Print Menu
                                 
                                 /**
                                 * @details				Waits for the passcode. If the correct passcode is pressed
                                 *						unlock by pulling pin 2 on PORTB HIGH
                                 * @param[in]	state	Needs to be cleared to indicate that the safe is unlocked
                                 * @return				void
                                 **/
                                 Ulock:
000101 2788                      	CLR state				;Clear state register
000102 e0ea                      	LDI ZL,low(enter_pass<<1)	;Load the enter pass message address
000103 e0f1                      	LDI ZH,high(enter_pass<<1)
000104 dfe4                      	RCALL Send_byte				;Print the enter pass message
000105 c013                      	RJMP Get_bytes				;Get pass from user and check if the pass entered is correct
                                 	Ulock_it:
000106 e3ee                      		LDI ZL,low(unlock_it<<1)	;Load unlock message address
000107 e0f1                      		LDI ZH,high(unlock_it<<1)
000108 dfe0                      		RCALL Send_byte			;Print the unlock message
000109 b985                      		OUT PORTB, state		;Pull all pins LOW
00010a e022                      		LDI r18, (1<<1)			;Pull pin 9 HIGH
00010b b925                      		OUT PORTB,r18			
00010c cfc5                      	RJMP Main
                                 
                                 /**
                                 * @details				Waits for the passcode. If correct, waits for the new passcode.
                                 *						Stores new passcode in SRAM
                                 * @param[in]	state	Needs to be set to indicate that the safe is being reset
                                 * @return				void
                                 **/
                                 RESET:
00010d ef8f                      	LDI state,0xFF				;Set state register to indicate reset mode
00010e e0ea                      	LDI ZL,low(enter_pass<<1)	;Load enter pass message address
00010f e0f1                      	LDI ZH,high(enter_pass<<1)
000110 dfd8                      	RCALL Send_byte				;Print enter pass message
                                 
000111 c007                      	RJMP Get_bytes				;Check if entered pass is correct
                                 	Reset_it:
000112 e024                      		LDI r18,(1<<2)
000113 b925                      		OUT PORTB,r18
000114 d01f                      		RCALL Get_new_pass		;Get the new pass
000115 2722                      		CLR r18
000116 b925                      		OUT PORTB,r18
000117 cfe1                      	RJMP Lock
000118 cfb9                      	RJMP Main
                                 
                                 /**
                                 * @details				Waits for each individual byte moves them into storage registers	
                                 * @return				void
                                 **/
                                 Get_bytes:
000119 dfc4                      	RCALL Uart_Rx				;Wait for first byte of pass
00011a 2f30                      	MOV r19,out_buf				;Store first byte
00011b dfc2                      	RCALL uart_Rx				;Wait for byte 2
00011c 2f40                      	MOV r20,out_buf				;Store byte 2
00011d dfc0                      	RCALL Uart_Rx				;Wait for byte 3
00011e 2f50                      	MOV r21,out_buf				;Store byte 3
00011f dfbe                      	RCALL Uart_Rx				;Wait for byte 4
000120 2f60                      	MOV r22,out_buf				;Store byte 4
                                 
000121 c001                      	RJMP Check_pass				;Check if the pass is correct
000122 cfaf                      	RJMP Main
                                 
                                 /**
                                 * @details				Checks if the entered pass matches the stored pass
                                 * @params[in]	state	Branch depending on if state is cleared or set
                                 * @return				void
                                 **/
                                 Check_pass:
000123 e0f2                      	LDI ZH, high(pass<<1)		;Load the address of the pass in SRAM
000124 e0e0                      	LDI ZL, low(pass<<1)
                                 	COMP:
000125 9171                      		LD r23,Z+				;Load a byte from SRAM as poited to by Z (post incremented)
000126 1773                      		CP r23,r19				;Compare the first bytes of the password with the user input
000127 f669                      			BRNE Error			;Throw an error if they are not equal
000128 9171                      		LD r23,Z+
000129 1774                      		CP r23,r20
00012a f651                      			BRNE Error
00012b 9171                      		LD r23,Z+
00012c 1775                      		CP r23,r21
00012d f639                      			BRNE Error
00012e 8170                      		LD r23,Z
00012f 1776                      		CP r23,r22
000130 f621                      			BRNE Error
                                 
000131 ff81                      		SBRS state,1			;If bit in register is clear
000132 cfd3                      			RJMP Ulock_it		;the unlock subroutine called it
000133 cfde                      		RJMP Reset_it			;Else, the reset subroutine called it
                                 
                                 /**
                                 * @details				Gets the new pass and stores each byte into a register
                                 * @return				void
                                 **/
                                 Get_new_pass:
000134 e0f1                      	LDI ZH, high(new_pass<<1) ;Ask user to enter new pass
000135 e1ec                      	LDI ZL, low(new_pass<<1)
000136 dfb2                      	RCALL Send_byte
                                 
000137 dfa6                      	RCALL Uart_Rx			;Process each key input
000138 2f30                      	MOV r19,out_buf			;Store key inputs is seperate registers
000139 dfa4                      	RCALL uart_Rx
00013a 2f40                      	MOV r20,out_buf
00013b dfa2                      	RCALL Uart_Rx
00013c 2f50                      	MOV r21,out_buf
00013d dfa0                      	RCALL Uart_Rx
00013e 2f60                      	MOV r22,out_buf
                                 		
00013f d001                      	RCALL Store_pass			;Store pass is SRAM
000140 9508                      	RET
                                 
                                 /**
                                 * @details				Store new pass in SRAM
                                 * @param[in]	R19:R22	Bits containing the new passcode
                                 * @return				void
                                 **/
                                 Store_pass:
000141 e0d2                      	LDI YH,high(pass<<1)		;Load address 
000142 e0c0                      	LDI YL,low(pass<<1)
                                 
000143 9339                      	ST Y+,r19					;Store each byte in SRAM at the address poited to by Y (post increment)
000144 9349                      	ST Y+,r20
000145 9359                      	ST Y+,r21
000146 8368                      	ST Y,r22
                                 
000147 9508                      	RET


